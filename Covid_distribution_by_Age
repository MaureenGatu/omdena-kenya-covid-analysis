        "def data_table(df):\n",
        "  df = df.set_index('Age_group')\n",
        "  age_cases = df.T\n",
        "  age_cases = age_cases.reset_index()\n",
        "  age_cases = age_cases.rename(columns=({'index': 'Date'}))\n",
        "  start = full_text.find(' AS AT ') +7\n",
        "  end = full_text.find(' DAY:', start)\n",
        "  date = full_text[start:end]\n",
        "  col = 'Fatalities' + date\n",
        "  age_cases['Date'] = date\n",
        "  age_cases = age_cases.set_index('Date')\n",
        "  return age_cases\n",
        "\n",
        "# data_table(new_cases)"
